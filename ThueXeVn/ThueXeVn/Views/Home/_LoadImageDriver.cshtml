@if (Model != null)
{
    if (Model.Count > 0)
    {
        <div class="col-xs-12 col-md-12 hidden-xs margintop10">
            <div class="flexslider sectionMarginTop album_driver" style="margin-bottom: 0; border: 0;">
                <ul class="slides">
                    @foreach (var item in Model)
                    {
                        <li data-thumb="@item.img_url">
                            <div style="position: relative;">
                                <img src="@item.img_url" class="item_img" style="height: 350px;" />
                            </div>
                        </li>
                    }

                </ul>
            </div>
        </div>
        
    }

}


