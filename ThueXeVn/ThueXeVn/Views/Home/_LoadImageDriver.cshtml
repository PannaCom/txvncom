@model List<ThueXeVn.Models.driver_images>

@if (Model != null)
{
    if (Model.Count > 0)
    {
        <ul class="bxslider" id="captioned-gallery" style="margin-left: 0; margin-bottom: 10px;">
            @foreach (var item in Model)
            {
                <li><img src="@item.img_url" /></li>
            }
        </ul>
    }
}


