@model List<ThueXeVn.Models.driver_images_vm>


@if (Model != null)
{
    if (Model.Count > 0)
    {
        <div class="margintop10 text-center">
            <div class="flexslider sectionMarginTop album_driver" style="margin-bottom: 0; border: 0;">
                <ul class="slides">
                    @foreach (var item in Model)
                    {
                        { Html.RenderAction("LoadCarImage", new { made = @item.made, model = @item.model }); }
                        if (item.id != 0)
                        {
                            <li data-thumb="@item.img_url">
                                <div style="position: relative;">
                                    <img src="@item.img_url" class="img-responsive" style="width: 100%; height: 120px;" />
                                </div>
                            </li>
                        }
                    }

                </ul>
            </div>
        </div>        
    }

}


