@model List<ThueXeVn.Models.driver_images>
@using ThueXeVn;



@if (Model != null)
{
    if (Model.Count > 0)
    {
        <div class="row profile--item">
            <div class="col-md-3">
                <h2 class="item_label">Hình ảnh:</h2>
            </div>
            <div class="col-md-9">

                <!-- Place somewhere in the <body> of your page -->
                <div id="carousel" class="gallery_driver">
                    @foreach (var item in Model)
                    {
                        <div class="gallery_product col-lg-3 col-md-3 col-sm-3 col-xs-6 filter hdpe">
                            <img src="@item.img_url" class="img-responsive img-driver-item">
                        </div>
                    }
                </div>
            </div>
        </div>
        
    }
}
