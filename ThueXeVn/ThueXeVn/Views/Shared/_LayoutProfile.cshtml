<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="description" content="@Html.Raw(@ViewBag.des)">
    <meta name="keywords" content="@Html.Raw(@ViewBag.keywords)" />
    <meta name="robots" content="index,follow,noodp" />
    <meta name="author" content="ThueXeVn.com" />
    <meta property="og:locale" content="vi_VN" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="@Html.Raw(ViewBag.url)" />
    <meta property="og:title" content="@Html.Raw(@ViewBag.title)" />
    <meta property="og:description" content="@Html.Raw(@ViewBag.des)" />
    <meta property="og:image" itemprop="thumbnailUrl" content="@ViewBag.image" />
    <meta property="fb:admins" content="100006557590947" />

    <!-- Favicons
    ================================================== -->
    <link rel="shortcut icon" href="~/Images/page/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/Images/page/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/Images/page/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/Images/page/apple-touch-icon-114x114.png">
    <link href="~/Content/fontend/css/fonts.css" rel="stylesheet" />
    <!-- Bootstrap -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!-- Jquery UI style
    ================================================== -->
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/fontend/css/main")
    @RenderSection("Styles", false)

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @Scripts.Render("~/bundles/modernizr")

    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>*@
    <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.0/jquery-ui.min.css"></script>*@
    <script src="~/Scripts/jquery-ui-1.12.0.min.js"></script>
    <script src="~/Scripts/jquery.datetimepicker.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/toastr/toastr.min.css" rel="stylesheet" />
    <script src="~/Content/toastr/toastr.min.js"></script>
    @Scripts.Render("~/Scripts/core")


    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-83915618-1', 'auto');
        ga('send', 'pageview');
    </script>
</head>
<body>
    <div id="pageContainer">

        <!-- Main Navigation
        ================================================== -->
        @Html.Partial("_mainMenu")


        <!-- Home Section
        ================================================== -->
        @RenderBody()
    </div>
    <!-- Footer
    ================================================== -->
    @Html.Partial("_Footer")



    @*<script src="http://maps.googleapis.com/maps/api/js?sensor=false&amp;libraries=places"></script>*@
    @*<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDLPSKQ4QV4xGiQjnZDUecx-UEr3D0QePY&libraries=places" type="text/javascript"></script>*@
    <script src="~/Scripts/jquery.geocomplete.min.js"></script>

    @Scripts.Render("~/Content/fontend/js/scripts")

    @RenderSection("scripts", required: false)
</body>
</html>
